{
    "name": "monolith/collections",
    "description": "Collection classes for the Monolith framework.",
    "keywords": [
        "monolith",
        "collections"
    ],
    "authors": [
        {
            "name": "Shawn McCool",
            "email": "shawn@heybigname.com"
        },
        {
            "name": "Mitchell van Wijngaarden",
            "email": "mitchell@kooding.nl"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Monolith\\Collections\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Monolith\\Collections\\": "spec/"
        },
        "files": [
            "spec/functions.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
