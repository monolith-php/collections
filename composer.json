{
  "name": "monolith/collections",
  "description": "Collection classes for the Monolith framework.",
  "keywords": [
    "monolith",
    "collections"
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\Collections\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\Collections\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
